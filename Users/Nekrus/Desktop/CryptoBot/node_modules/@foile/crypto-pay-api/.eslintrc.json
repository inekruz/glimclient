{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "eslint:recommended"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 1,
        "prettier/prettier": 2
    }
}
